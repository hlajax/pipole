<% if current_lecteur.liked? @article %>
   $(".btn-primary").addClass("active");
<% else %>
   $(".btn-primary").removeClass("disabled");
<% end %>
   $("#art-<%= @article.id %>").html('<small><%= @article.get_upvotes.size %> <%= @article.agree %> | <%= @article.get_downvotes.size %> <%= @article.disagree %></small>');